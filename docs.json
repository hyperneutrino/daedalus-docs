{
    "$schema": "https://mintlify.com/docs.json",
    "theme": "mint",
    "name": "Daedalus",
    "colors": {
        "primary": "#009688",
        "light": "#11c0af",
        "dark": "#0c6d63"
    },
    "favicon": "https://daedalusbot.xyz/favicon.ico",
    "navigation": {
        "global": {
            "anchors": [
                {
                    "anchor": "Main Site",
                    "icon": "screwdriver-wrench",
                    "href": "https://daedalusbot.xyz"
                }
            ]
        },
        "tabs": [
            {
                "tab": "Guides",
                "groups": [
                    {
                        "group": "Getting started",
                        "pages": ["index", "terms", "privacy"]
                    },
                    {
                        "group": "Documentation",
                        "pages": ["permissions", "dev-mode", "custom-messages", "premium", "cohosting", "security"]
                    }
                ]
            },
            {
                "tab": "Module List",
                "groups": [
                    {
                        "group": "Modules",
                        "pages": [
                            "modules/index",
                            "modules/logging",
                            "modules/welcome",
                            "modules/supporter-announcements",
                            "modules/xp",
                            "modules/reaction-roles",
                            "modules/moderation",
                            "modules/starboard",
                            "modules/automod",
                            "modules/account-filter",
                            "modules/sticky-roles",
                            "modules/autoroles",
                            "modules/custom-roles",
                            "modules/stats-channels",
                            "modules/autoresponder"
                        ]
                    },
                    {
                        "group": "Commands",
                        "pages": [
                            {
                                "group": "XP",
                                "pages": [
                                    "commands/top",
                                    "commands/rank",
                                    { "group": "/xp", "pages": ["commands/xp/import", "commands/xp/reset-user", "commands/xp/reset-all"] }
                                ]
                            },
                            {
                                "group": "Moderation",
                                "pages": [
                                    "commands/ban",
                                    "commands/kick",
                                    "commands/mute",
                                    "commands/timeout",
                                    "commands/warn",
                                    "commands/note",
                                    "commands/unban",
                                    "commands/unmute",
                                    "commands/mod-view",
                                    {
                                        "group": "/massban",
                                        "pages": [
                                            "commands/massban/list",
                                            "commands/massban/url",
                                            "commands/massban/file",
                                            "commands/massban/import",
                                            "commands/massban/export"
                                        ]
                                    },
                                    "commands/view-history",
                                    "commands/toggle-history-entry",
                                    "commands/slowmode",
                                    "commands/purge"
                                ]
                            },
                            {
                                "group": "Account Filter",
                                "pages": [
                                    {
                                        "group": "/account-filter",
                                        "pages": ["commands/account-filter/exempt", "commands/account-filter/reset", "commands/account-filter/reset-all"]
                                    }
                                ]
                            },
                            { "group": "Custom Roles", "pages": ["commands/custom-role"] }
                        ]
                    }
                ]
            }
        ]
    },
    "logo": { "dark": "/logo/dark.svg", "light": "/logo/light.svg" },
    "navbar": {
        "links": [
            {
                "label": "Support",
                "href": "https://discord.gg/7TRKfSK7EU"
            }
        ],
        "primary": {
            "type": "button",
            "label": "Dashboard",
            "href": "https://dashboard.mintlify.com"
        }
    },
    "footer": {
        "socials": {
            "github": "https://github.com/hyperneutrino",
            "discord": "https://discord.gg/7TRKfSK7EU"
        }
    },
    "description": "Daedalus, the modern all-purpose Discord bot",
    "background": {
        "color": {
            "light": "#FFFFFF",
            "dark": "#000000"
        },
        "decoration": "windows"
    },
    "appearance": {
        "default": "system"
    },
    "fonts": {
        "family": "Fira Sans",
        "weight": 300
    },
    "errors": {
        "404": {
            "title": "404 Not Found",
            "description": "The URL you entered does not point to a valid page."
        }
    },
    "interaction": {
        "drilldown": true
    },
    "metadata": {
        "timestamp": true
    },
    "seo": {
        "indexing": "all"
    }
}
